worker_processes  16;

events {
    worker_connections 2048;
}

stream {
    upstream graylogserversudp {
GRAYLOGSERVERSUDP
    }

    server {
        listen 12201 udp;
        proxy_pass graylogserversudp;
    }

    upstream graylogserverstcp {
GRAYLOGSERVERSTCP
    }

    server {
        listen 1514;
        proxy_pass graylogserverstcp;
    }

}
